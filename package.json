    "version": "0.0.1",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "npm run copy-dts && tsc --build tsconfig-no-spec.json && npm run build:css && babel lib -d lib",
        "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
        "build-ts:dts-only": "tsc --declaration --emitDeclarationOnly --project tsconfig-no-spec.json",
        "build:css": "npm run build:css:generate && npm run build:css:clean",
        "build:css:generate": "",
        "build:css:clean": "rimraf ./css",
        "build:css:script": "tsc --project ./scripts/tsconfig.json",
        "lint": "eslint --cache --cache-location node_modules/.cache/ \"./**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
        "prebuild": "rimraf lib",
        "prepublish": "npm run test && npm run build",
        "prepublishOnly": "npm run lint && npm test",
        "test": "",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\""
    },
    "publishConfig": {
        "access": "restricted"
    },
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@babel/cli": "7.14.3",
        "@babel/core": "7.14.3",
        "@babel/plugin-proposal-object-rest-spread": "7.10.4",
        "@babel/plugin-proposal-optional-chaining": "^7.14.2",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/plugin-transform-runtime": "^7.14.3",
        "@babel/preset-env": "7.10.4",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.2",
        "@babel/register": "7.10.5",
        "@linaria/babel-preset": "^3.0.0-beta.13",
        "@linaria/webpack-loader": "^5.0.3",
        "@storybook/addon-actions": "6.4.10",
        "@storybook/addon-essentials": "6.4.10",
        "@storybook/addon-links": "6.4.10",
        "@storybook/react": "6.5.16",
        "@types/node": "14.0.24",
        "babel-loader": "^8.2.2",
        "babel-plugin-lodash": "3.3.4",
        "copyfiles": "^2.4.1",
        "rimraf": "^3.0.2",
        "ts-loader": "^8.0.3",
        "webpack": "^5.38.1",
        "webpack-cli": "4.9.0"
    },
    "peerDependencies": {
        "react": ">=17.0.2 <19",
        "react-dom": ">=17.0.2 <19"
    },
    "dependencies": {
        "@linaria/react": "^5.0.3",
        "axios": "^1.4.0",
        "classnames": "^2.3.2",
        "react": ">=17.0.2 <19",
        "react-dom": ">=17.0.2 <19"
    }
}
