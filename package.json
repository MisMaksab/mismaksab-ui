{
    "name": "mismaksab-ui",
    "description": "",
    "keywords": [],
    "author": "grigorevpv@gmail.com (Pavel Grigorev)",
    "license": "ISC",
    "version": "0.0.4",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "npm run copy-dts && npm run copy-assets && tsc --build tsconfig-no-spec.json && npm run build:css && babel lib -d lib",
        "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
        "copy-assets": "copyfiles -u 1 \"src/assets/**/*\" lib",
        "build:package": "rm -rf lib && tsc --outDir lib && babel src --out-dir lib --extensions .tsx,.ts && linaria -o lib --source-root src --insert-css-requires lib \"src/**/*.ts\" --modules=es2015 && npm run copy-assets",
        "build:package-windows": "rd /s /q lib && tsc --outDir lib && babel src --out-dir lib --extensions .tsx,.ts && linaria -o lib --source-root src --insert-css-requires lib \"src/**/*.ts\" --modules=es2015 && npm run copy-assets",
        "build-ts:dts-only": "tsc --declaration --emitDeclarationOnly --project tsconfig-no-spec.json",
        "build:css": "npm run build:css:generate && npm run build:css:clean",
        "build:css:generate": "",
        "build:css:clean": "rimraf ./css",
        "build:css:script": "tsc --project ./scripts/tsconfig.json",
        "lint": "",
        "lint:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
        "prebuild": "rimraf lib",
        "prepublish": "npm run test && npm run build",
        "prepublishOnly": "npm run lint && npm test",
        "test": "",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\""
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@linaria/babel-preset": "^4.1.2",
        "@linaria/cli": "^4.1.2",
        "@linaria/core": "^4.1.1",
        "@linaria/react": "^4.1.2",
        "@linaria/stylelint": "^4.1.3",
        "@linaria/webpack-loader": "^5.0.3",
        "@storybook/addon-actions": "6.4.10",
        "@storybook/addon-essentials": "6.4.10",
        "@storybook/addon-links": "6.4.10",
        "@storybook/react": "6.5.16",
        "@stylelint/postcss-css-in-js": "^0.38.0",
        "@testing-library/react": "^13.4.0",
        "@types/babel__core": "^7.1.19",
        "@types/jest": "^29.1.2",
        "@types/node": "14.0.24",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "babel-jest": "^29.1.2",
        "babel-loader": "^8.2.2",
        "babel-plugin-lodash": "3.3.4",
        "browserslist": "^4.21.3",
        "copyfiles": "^2.4.1",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.1",
        "postcss": "^8.4.16",
        "postcss-syntax": "^0.36.2",
        "prettier": "^2.7.1",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "rimraf": "^3.0.2",
        "sha1": "^1.1.1",
        "stylelint": "^14.11.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended": "^9.0.0",
        "ts-loader": "^8.0.3",
        "typescript": "^4.7.4",
        "yarn-release": "^1.10.3"
    },
    "peerDependencies": {
        "react": ">=17.0.2 <19",
        "react-dom": ">=17.0.2 <19"
    },
    "dependencies": {
        "@babel/runtime": "^7.18.9",
        "axios": "^1.4.0",
        "classnames": "^2.3.2"
    }
}
