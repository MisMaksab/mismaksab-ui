{
  "name": "mismaksab-ui",
  "description": "Common ui library for MisMaksab project",
  "version": "1.0.10",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grigorevpv/mismaksab-ui.git"
  },
  "keywords": [
    "ui"
  ],
  "author": "Pavel Grigorev (grigorevpv@gmail.com)",
  "license": "ISC",
  "private": false,
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "homepage": "https://grigorevpv.github.io/mismaksab-ui",
  "bugs": {
    "url": "https://github.com/grigorevpv/mismaksab-ui/issues"
  },
  "scripts": {
    "build": "npm run copy-dts && tsc --build tsconfig.json && npm run build:css && babel lib -d lib",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
    "build-ts:dts-only": "tsc --declaration --emitDeclarationOnly --project tsconfig-no-spec.json",
    "build:css": "npm run build:css:generate && npm run build:css:clean",
    "build:css:generate": "",
    "build:css:clean": "rimraf ./css",
    "build:css:script": "tsc --project ./scripts/tsconfig.json",
    "lint": "eslint --cache --cache-location node_modules/.cache/ \"./**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
    "prebuild": "rimraf lib",
    "prepublish": "npm run test && npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "test": "",
    "storybook": "start-storybook -p 6006",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "build-storybook": "storybook build",
    "predeploy": "yarn build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/register": "7.10.5",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/rollup": "^8.0.1",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-loader": "^9.1.2",
    "babel-plugin-lodash": "3.3.4",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "gh-pages": "^5.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "storybook": "^7.0.7",
    "style-loader": "^3.3.3",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.3",
    "webpack": "^5.38.1",
    "webpack-cli": "4.9.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2 <19",
    "react-dom": ">=17.0.2 <19"
  },
  "dependencies": {
    "@linaria/react": "^5.0.3",
    "axios": "^1.4.0",
    "classnames": "^2.3.2"
  }
}
